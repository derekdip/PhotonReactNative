      "package": "com.derekpenaloza.maze",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "430f1e34-c335-4985-90be-3e3cd0f57b5f"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "ios_app_id": "ca-app-pub-3663356470470158~4914744704",
    "android_app_id": "ca-app-pub-3663356470470158~7505835758"
  }
